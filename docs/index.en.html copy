  &quot;high&quot;: <i style="color: black;">[High]</i>,
  &quot;low&quot;: <i style="color: black;">[Low]</i>,
  &quot;close&quot;: <i style="color: black;">[Close]</i>,
  &quot;volume&quot;: <i style="color: black;">[Volume]</i>,
  &quot;time&quot;: <i style="color: black;">[Time]</i>
}
</code></p>

